<%- if controller_name != 'sessions' %>
  <%= link_to "Entrar", new_session_path(resource_name), class: "btn btn-primary float-right" %>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Cadastrar", new_registration_path(resource_name), class: "btn btn-primary float-right" %>
<% end %>
